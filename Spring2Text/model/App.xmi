<?xml version="1.0" encoding="UTF-8"?>
<springModel:Application
  xmlns:xmi="http://www.omg.org/XMI"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:springModel="http://www.example.org/springModel"
  xsi:schemaLocation="http://www.example.org/springModel springModel.ecore"
  name="App"
>
      <properties>
        <property
            name="spring.datasource.url"
            value="jdbc:mysql://localhost:3306/mydb?serverTimezone=Africa/Algiers"/>
        <property
            name="spring.datasource.username"
            value="root"/>
        <property
            name="spring.datasource.password"
            value="user"/>
        <property
            name="spring.datasource.driver-class-name"
            value="com.mysql.cj.jdbc.Driver"/>
        <property
            name="spring.jpa.hibernate.ddl-auto"
            value="update"/>
        <property
            name="spring.jpa.show-sql"
            value="true"/>
        <property
            name="spring.jpa.hibernate.naming-strategy"
            value="org.hibernate.cfg.ImprovedNamingStrategy"/>
        <property
            name="spring.jpa.properties.hibernate.dialect"
            value="org.hibernate.dialect.MySQL5InnoDBDialect"/>
        <property
            name="spring.jpa.properties.hibernate.show_sql"
            value="true"/>
        <property
            name="spring.jpa.properties.hibernate.use_sql_comments"
            value="true"/>
        <property
            name="spring.jpa.properties.hibernate.format_sql"
            value="true"/>
        <property
            name="spring.jpa.properties.hibernate.type"
            value="trace"/>
        <property
            name="spring.jpa.properties.hibernate.enable_lazy_load_no_trans"
            value="true"/>
        <property
            name="server.port"
            value="8083"/>
      </properties>
      <entity
          name="Product">
        <field
            xsi:type="springModel:BaseField"
            name="productId"
            type="String"/>
        <field xsi:type="springModel:BaseField" name="productName"
	type="String" />
        <field
            xsi:type="springModel:BaseField"
            name="description"
            type="String"/>
        <field
            xsi:type="springModel:BaseField"
            name="price"
            type="double"/>
        <field
            xsi:type="springModel:SubClass"
            name="productCategory"
            type="ProductCategory"
            manyToOne="true"/>
        <field
            xsi:type="springModel:BaseField"
            name="availableItemCount"
            type="int"/>
      </entity>
      <entity
          name="ProductCategory">
        <field
            xsi:type="springModel:BaseField"
            name="productCategoryId"
            type="String"/>
        <field
            xsi:type="springModel:BaseField"
            name="productCategoryName"
            type="String"/>
        <field
            xsi:type="springModel:BaseField"
            name="description"
            type="String"/>
        <field
            xsi:type="springModel:SubClass"
            name="products"
            type="List&lt;Product>"
            oneToMany="true"/>
      </entity>
      <entity
          name="Review">
        <field
            xsi:type="springModel:BaseField"
            name="reviewId"
            type="String"/>
        <field
            xsi:type="springModel:BaseField"
            name="productId"
            type="String"/>
        <field
            xsi:type="springModel:BaseField"
            name="userId"
            type="String"/>
        <field
            xsi:type="springModel:BaseField"
            name="userName"/>
        <field
            xsi:type="springModel:BaseField"
            name="ratingValue"
            type="double"/>
        <field
            xsi:type="springModel:BaseField"
            name="reviewMessage"
            type="String"/>
      </entity>
</springModel:Application>
